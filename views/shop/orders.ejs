<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.length > 0) {%>
                <ul >
                    <% orders.forEach(order => {%>
                        <li>
                            <h3># <%= order._id %></h3>
                            <ul>
                                <% order.products.forEach(item => { %>
                                    <li><%= item.product.title%> (<%= item.quantity %>)</li>
                                <% }) %>
                            </ul>
                        </li>
                    <% }) %>
                </ul>
            <% } else {%>
                <h1>Nothing there!</h1>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>